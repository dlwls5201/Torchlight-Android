<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="text" format="reference|string" />

    <declare-styleable name="CustomThreeBtn" />

    <declare-styleable name="ItemSelectorView">
        <attr name="multi_selectable" format="boolean" />
        <attr name="removable" format="boolean" />
        <attr name="all_deselectable" format="boolean" />
        <attr name="selected_color" format="color" />
        <attr name="deselected_color" format="color" />
        <attr name="selected_text_color" format="color" />
        <attr name="deselected_text_color" format="color" />
        <attr name="selected_bottom_text_color" format="color" />
        <attr name="deselected_bottom_text_color" format="color" />
        <attr name="selected_icon_color" format="color" />
        <attr name="deselected_icon_color" format="color" />
        <attr name="border_color" format="color" />
        <attr name="border_thickness" format="integer" />
        <attr name="remove_btn_bg_color" format="color" />
        <attr name="remove_btn_x_color" format="color" />
        <attr name="remove_btn_size" format="dimension" />
        <attr name="remove_btn_margin" format="dimension" />
        <attr name="remove_btn_border_color" format="color" />
        <attr name="remove_btn_border_thickness" format="color" />
        <attr name="radius" format="dimension" />
        <attr name="text_size" format="dimension" />
        <attr name="icon_size" format="dimension" />
        <attr name="max_select_count" format="integer" />
        <attr name="item_margin" format="dimension" />
        <attr name="item_padding" format="dimension" />
        <attr name="scrollable" format="boolean" />
        <attr name="display_type">
            <flag name="text" value="1" />
            <flag name="icon" value="2" />
        </attr>
    </declare-styleable>
</resources>